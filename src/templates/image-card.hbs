{{#each this}}  
  <div class="photo-card">    
    <a class="link-card" href="{{this.largeImageURL}}">
    <img class="img-card" src="{{this.webformatURL}}" alt="{{this.tags}}" loading="lazy" />  
    </a>  
    <div class="info">
      <p class="info-item">
        <b>Likes</b>
        <span>{{this.likes}}</span>
      </p>
      <p class="info-item">
        <b>Views</b>
        <span>{{this.views}}</span>
      </p>
      <p class="info-item">
        <b>Comments</b>
        <span>{{this.comments}}</span>
      </p>
      <p class="info-item">
        <b>Downloads</b>
        <span>{{this.downloads}}</span>
      </p>
    </div>
  </div>  
{{/each}}

